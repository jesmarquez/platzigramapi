{
  "name": "platzigramapi",
  "version": "1.0.0",
  "description": "Platzigram REST",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && ava test/**-test.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "NODE_ENV": "test"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesmarquez/platzigramapi.git"
  },
  "author": "Jesus Marquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesmarquez/platzigramapi/issues"
  },
  "homepage": "https://github.com/jesmarquez/platzigramapi#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "standard": "^8.6.0",
    "test-listen": "^1.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "http-hash": "^2.0.0",
    "micro": "^6.2.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "uuid-base62": "^0.1.0"
  }
}
